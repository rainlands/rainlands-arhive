{
  "presets": ["stage-2"],
  "plugins": [
    "transform-class-properties",
    "transform-export-extensions",
    ["module-resolver", {
      "alias": {
        "@core": "./src/core",
        "@resources": "./src/resources",
        "@utils": "./src/utils",
        "!constants": "./src/constants.js",
        "@root": "./src",
        "chunked-terrain-generator": "../chunked-terrain-generator",
        "ctg-plugin-detailer": "../chunked-terrain-generator/plugins/detailer"
      }
    }]
  ]
}
