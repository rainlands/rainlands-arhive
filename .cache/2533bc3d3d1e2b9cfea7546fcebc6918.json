nt.body.appendChild(stats.domElement);\n\n    this.stats = stats;\n\n    this.scene = new THREE.Scene();\n    this.scene.fog = new THREE.Fog(0xffffff, 0, 200);\n\n    if (process.env.NODE_ENV !== 'production') {\n      this.scene.add(new THREE.AxesHelper(15));\n    }\n\n    this.world = new CANNON.World();\n    this.world.gravity.set(0, -9.81, 0);\n    this.world.broadphase = new CANNON.NaiveBroadphase();\n\n    this.objects = [];\n\n    window.addEventListener('visibilitychange', this.onVisibilityChange);\n    window.addEventListener('focus', this.onFocus);\n  }\n\n  // //////////\n  // EVENTS //\n  // //////////\n\n  onVisibilityChange = () => {\n    if (document.hidden) {\n      this.unfocusTime = performance.now();\n    }\n  };\n\n  // ///////////\n  // GETTERS //\n  // ///////////\n\n  get entity() {\n    return this.scene;\n  }\n\n  // ///////////\n  // METHODS //\n  // ///////////\n\n  addElement = element => this.scene.add(element);\n\n  addObject = (object) => {\n    this.objects.push(object);\n    this.scene.add(object.mesh);\n    this.world.add(object.body);\n  };\n\n  update = () => {\n    this.stats.begin();\n\n    if (this.unfocusTime) {\n      const focusTime = -(this.unfocusTime - performance.now());\n\n      this.world.step(1 / 60, focusTime);\n\n      this.unfocusTime = null;\n    } else {\n      this.world.step(1 / 60);\n    }\n\n    this.objects.forEach(object => object.updatePosition());\n\n    this.stats.end();\n  };\n}\n"},"lineCount":null}},"hash":"713da38e4e1f451303663cf965615479","cacheData":{"env":{"NODE_ENV":"development"}}}